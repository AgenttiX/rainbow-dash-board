{
    "extends": "eslint:recommended",
    "env": {
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 2017
    },
    "rules": {
        "curly": ["error", "multi-line"],
        "comma-dangle": ["error", "always-multiline"],
        "semi": ["error", "always"],
        "no-process-env": "error",
        "no-console": "error",
        "max-len": ["error", { "code": 80 }]
    }
}
